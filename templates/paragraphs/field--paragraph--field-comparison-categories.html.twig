      <div class="tab-items-list">
              <ul class="resp-tabs-list">
                {% for item in items %}
                <li class="resp-tab-item">
                  {{ loop.index }} -  {{ item.content['#paragraph'].field_tab_name.value }}
                  </li>
                {% endfor %}   
              </ul>
            </div>
            <div class="resp-tabs-container">
              {% for item in items %}              
                <div class="resp-tabs-container-item">
                  <div class="prod-tab-content">
                    <div class="info-outer">
                      <div class="left">
                      
                        <h4><img src="{{ active_theme_url }}/src/img/nu.png"></h4>
                        <ul>
                            {% for item in item.content['#paragraph'].field_what_nu_offers %}
                              <li>
                              <img src="{{ active_theme_url }}/src/img/tick.png"> <span>{{item.value}}</span>
                              </li>
                            {% endfor %}
                        </ul>  
                      </div>
                      <div class="right">
                        <h4>Others</h4>
                        <ul>
                        {% for item in item.content['#paragraph'].field_what_others_offer %}
                        <li>
                          <img src="{{ active_theme_url }}/src/img/close.png"> <span>{{item.value}}</span>
                        </li>
                        {% endfor %} 
                        </ul>
                      </div>
                    </div>
                  </div>
                </div>
              
              {% endfor %}
           </div>
   

      
      
      



