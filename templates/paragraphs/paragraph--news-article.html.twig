{% block paragraph %}
	<div class="news-article">
		{% block content %}
			{% if content.field_link[0]['#url'] is not empty %}
				<a href="{{ content.field_link[0]['#url'] }}" target=" _blank">
				{% endif %}

				{% if content.field_icons[0] is not empty %}
					<div class="news-icons">{{ content.field_icons }}</div>
				{% endif %}
				{% if content.field_description[0] is not empty %}
					<div class="description">{{ content.field_description }}</div>
				{% endif %}

				{% if content.field_link[0]['#url'] is not empty %}
				</a>
			{% endif %}
		{% endblock %}
	</div>
{% endblock paragraph %}
