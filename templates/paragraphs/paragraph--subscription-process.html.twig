{% block paragraph %}
	<div class="subscription-process">
		{% block content %}
			<div class="container">
				{% if content.field_description[0] is not empty %}
					<div class="description">{{ content.field_description }}</div>
				{% endif %}
				{% if content.field_iconbox[0] is not empty %}
					<div class="iconbox-wrapper">
						{% for key, iconbox in content.field_iconbox if key|first != '#' %}
							{{ iconbox }}
						{% endfor %}
					</div>
				{% endif %}
				{% if content.field_link[0] is not empty %}
					<div class="button-link">
						<a href="{{ content.field_link[0]['#url'] }}" {% if content.field_link['#options']['external'] %} target=" _blank" {% endif %} class="btn btn-white-arrow">
							{{ content.field_link[0]['#title'] }}
						</a>
					</div>
				{% endif %}
			</div>
		{% endblock %}
	</div>
{% endblock paragraph %}
